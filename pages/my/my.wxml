<!--用户信息  -->
<view class="container" style="padding:60rpx;background:#fff;position:relative;">
  <view  bindtap="bindViewTap" class="userinfo">
      <view class="user_circle2">
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      </view>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text> 
  </view>
</view>
<view class='all'>
    <view class="xuanze" bindtap="userinform">
        <view class="classname"><image src="../img/home-icon.png" mode="widthFix"></image>关于我们</view>
        <view class="nextImg">
            <image src="https://qncdn.playonwechat.com/shangcheng/next.png" mode="widthFix"/>
        </view> 
    </view>
</view>
